        "\n",
        "\n",
        "import whisper\n",
        "import google.generativeai as genai\n",
        "from google.colab import files\n",
        "from pydub import AudioSegment\n",
        "\n",
        "# 🔹 Step 1: Upload Audio File\n",
        "uploaded = files.upload()\n",
        "audio_file = list(uploaded.keys())[0]  # Get uploaded file name\n",
        "\n",
        "# 🔹 Step 2: Convert to WAV (if needed)\n",
        "def convert_audio(file_path):\n",
        "    if file_path.endswith(\".mp3\"):  # Convert MP3 to WAV for better accuracy\n",
        "        sound = AudioSegment.from_file(file_path)\n",
        "        sound = sound.set_channels(1).set_frame_rate(16000)  # Mono & 16kHz for better processing\n",
        "        new_path = \"converted_audio.wav\"\n",
        "        sound.export(new_path, format=\"wav\")\n",
        "        return new_path\n",
        "    return file_path  # If already WAV, use as-is\n",
        "\n",
        "audio_file_path = convert_audio(audio_file)\n",
        "\n",
        "# 🔹 Step 3: Transcribe with Whisper (Medium Model)\n",
        "def transcribe_audio(audio_path):\n",
        "    model = whisper.load_model(\"medium\")  # Using 'medium' for better speed & accuracy\n",
        "    result = model.transcribe(audio_path)\n",
        "    return result[\"text\"]\n",
        "\n",
        "transcribed_text = transcribe_audio(audio_file_path)\n",
        "print(\"\\n🔹 Whisper Transcription Output:\\n\", transcribed_text)\n",
        "\n",
        "# 🔹 Step 4: Refine with Gemini API\n",
        "genai.configure(api_key=\"Api key")  # Replace with your Gemini API key\n",
        "\n",
        "def refine_with_gemini(text):\n",
        "    prompt = f\"\"\"\n",
        "    Format the following transcription into a structured legal document.\n",
        "\n",
