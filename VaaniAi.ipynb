        "import whisper\n",
        "import google.generativeai as genai\n",
        "from google.colab import files\n",
        "from pydub import AudioSegment\n",
        "from gtts import gTTS\n",
        "import os\n",
        "\n",
        "# 🔹 Step 1: Generate and Save Greeting Message\n",
        "greeting_text = \"नमस्ते, आपका स्वागत है साक्षा चौपाल में। कृपया अपनी आवाज़ रिकॉर्ड करें।\"\n",
        "greeting_audio = \"greeting.mp3\"\n",
        "greeting_wav = \"greeting.wav\"  # Convert to WAV\n",
        "\n",
        "if not os.path.exists(greeting_audio):\n",
        "    tts = gTTS(text=greeting_text, lang=\"hi\")\n",
        "    tts.save(greeting_audio)\n",
        "\n",
        "# 🔹 Convert MP3 to WAV (if needed)\n",
        "sound = AudioSegment.from_file(greeting_audio, format=\"mp3\")\n",
        "sound.export(greeting_wav, format=\"wav\")\n",
        "\n",
        "# 🔹 Download Greeting Message for Local Playback\n",
        "print(\"🔹 Download the greeting message and play it locally.\")\n",
        "files.download(greeting_wav)\n",
        "\n",
        "# 🔹 Step 2: Upload Audio File\n",
        "print(\"🔹 Now, please upload your recorded audio.\")\n",
        "uploaded = files.upload()\n",
        "audio_file = list(uploaded.keys())[0]  # Get uploaded file name\n",
        "\n",
        "# 🔹 Step 3: Convert to WAV (if needed)\n",
        "def convert_audio(file_path):\n",
        "    if file_path.endswith(\".mp3\"):  # Convert MP3 to WAV for better accuracy\n",
        "        print(\"🔹 Converting MP3 to WAV for better transcription accuracy...\")\n",
        "        sound = AudioSegment.from_file(file_path)\n",
        "        sound = sound.set_channels(1).set_frame_rate(16000)  # Mono & 16kHz\n",
        "        new_path = \"converted_audio.wav\"\n",
        "        sound.export(new_path, format=\"wav\")\n",
        "        return new_path\n",
        "    return file_path  # If already WAV, use as-is\n",
        "\n",
        "audio_file_path = convert_audio(audio_file)\n",
        "\n",
        "# 🔹 Step 4: Transcribe with Whisper (Medium Model)\n",
        "def transcribe_audio(audio_path):\n",
        "    print(\"🔹 Transcribing audio using Whisper model...\")\n",
        "    model = whisper.load_model(\"medium\")  # Using 'medium' for better speed & accuracy\n",
        "    result = model.transcribe(audio_path)\n",
        "    return result[\"text\"]\n",
        "\n",
        "transcribed_text = transcribe_audio(audio_file_path)\n",
        "print(\"\\n🔹 Whisper Transcription Output:\\n\", transcribed_text)\n",
        "\n",
        "# 🔹 Step 5: Ask for the User's Name\n",
        "user_name = input(\"\\n🔹 कृपया अपना पूरा नाम दर्ज करें (यह दस्तावेज़ में शामिल किया जाएगा): \")\n",
        "\n",
        "# 🔹 Step 6: Refine with Gemini API\n",
        "genai.configure(api_key=\"AIzaSyDhVtDZE_abyXWzy_FSl1pV9bIzkth9kbU\")  # Replace with your Gemini API key\n",
        "\n",
        "def refine_with_gemini(text, name):\n",
        "    prompt = f\"\"\"\n",
        "    Format the following transcription into a structured legal document.\n",
        "\n",
        "    1. Correct grammar and spelling.\n",
        "    2. Add a proper legal structure.\n",
        "    3. Include placeholders for Date, Time, and Location.\n",
        "    4. Keep the meaning unchanged.\n",
        "    5. Mention the participant's name at the end.\n",
        "\n",
        "    Transcription:\n",
        "    {text}\n",
        "\n",
        "    Participant Name: {name}\n",
        "\n",
        "    Output a well-formatted legal document.\n",
        "    \"\"\"\n",
        "\n",
        "    response = genai.GenerativeModel(\"gemini-1.5-pro\").generate_content(prompt)\n",
        "    return response.text\n",
        "\n",
        "final_document = refine_with_gemini(transcribed_text, user_name)\n",
        "print(\"\\n🔹 Final Legal Document:\\n\", final_document)\n"
      ],
